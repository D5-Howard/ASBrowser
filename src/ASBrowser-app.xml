<?xml version="1.0" encoding="utf-8" standalone="no"?>
<application xmlns="http://ns.adobe.com/air/application/33.1">
    <id>com.d5power.asbrowser</id>
    <filename>ASBrowser</filename>
    <name>asbrowser</name>
    <versionNumber>1.0.0</versionNumber>
    <initialWindow>
        <content>[This value will be overwritten by Flash Builder in the output app.xml]</content>
        <title>ASBrowser</title>
        <systemChrome>none</systemChrome>
		<transparent>false</transparent>
        <resizable>false</resizable>
        <x>100</x>
        <y>100</y>
        <requestedDisplayResolution>high</requestedDisplayResolution> <!-- This also applies to Windows 10 also -->
        <depthAndStencil>true</depthAndStencil>
        <renderMode>direct</renderMode>
        <autoOrients>false</autoOrients>
        <fullScreen>true</fullScreen>
        <visible>true</visible>
    </initialWindow>
    <!--
    <icon>
        <image16x16>ico/icon16.png</image16x16>
        <image32x32>ico/icon32.png</image32x32>
        <image36x36>ico/icon36.png</image36x36>
        <image48x48>ico/icon48.png</image48x48>
    </icon>
    -->
    <supportedProfiles>extendedDesktop desktop</supportedProfiles>
    <extensions>
        <extensionID>com.tuarua.FreSharp</extensionID>
        <extensionID>com.tuarua.WebViewANE</extensionID>
        <extensionID>com.tuarua.FreSwift</extensionID>
    </extensions>
</application>
